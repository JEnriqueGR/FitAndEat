<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Fit&amp;Eat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitAndEat"
        tools:targetApi="31">
        <activity
            android:name=".SignInActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.FitAndEat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".StatsActivity" />
        <activity android:name=".CompleteProfileActivity" />
        <activity android:name=".UserGoalActivity" />
        <activity android:name=".NewSessionTrainActivity" />
        <activity android:name=".ExerciseDetailActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".AddFoodActivity" />
    </application>

</manifest>